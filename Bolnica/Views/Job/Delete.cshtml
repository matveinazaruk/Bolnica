@model Bolnica.Models.Job

@{
    ViewBag.Title = "Удаление вакансии";
}

<h2>Удаление вакансии</h2>

<h3>Вы действительно хотите удалить следующую вакансию?</h3>
<fieldset>
    <div class="display-field">
        @Html.DisplayFor(model => model.Name)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Удалить" /> |
        @Html.ActionLink("К списку вакансий", "Index")
    </p>
}
